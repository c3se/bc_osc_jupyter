#!/usr/bin/env bash

# Benchmark info
echo "TIMING - Starting main script at: $(date)"

# Set working directory to home directory
cd "${HOME}"

#
# Start Jupyter Notebook Server
#

# Benchmark info
echo "TIMING - Starting jupyter at: $(date)"

#
# Launch the Jupyter Notebook Server
set -x
# Option 1: module tree
#module purge
#module load GCCcore/10.2.0 JupyterLab Julia
#jupyter notebook --config="${CONFIG_FILE}"
# Option 2: standalone jupyterlab + extensions installed
#/apps/Jupyter/bin/jupyter notebook --config="${CONFIG_FILE}"
# Option 3: containers
#singularity exec --nv /apps/hpc-ai-containers/<= context.container => jupyter notebook
export CONTAINER=/apps/hpc-ai-containers/TensorFlow/TensorFlow_v2.3.1-tf2-py3-GPU-Jupyter.sif
singularity exec --nv $CONTAINER jupyter notebook --config="${CONFIG_FILE}"
